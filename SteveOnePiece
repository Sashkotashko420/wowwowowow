local Mercury = loadstring(game:HttpGet("https://raw.githubusercontent.com/deeeity/mercury-lib/master/src.lua"))()


local MainGUI = Mercury:Create{
    Name = "| Oblivion Hub |",
    Theme = Mercury.Themes.Dark,
    Link = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name
}

MainMainGUI:Credit{
	Name = "Bruno",
	Description = "Main Scripter",
	V3rm = "https://v3rmillion.net/member.php?action=profile&uid=2678717",
	Discord = "LxstAppleDev#0715"
}

MainGUI:Credit{
	Name = "Project Oblivion",
	Description = "Official Project Oblivion Discord Server",
	Discord = "discord.com/invite/cKsuBrYBzA"
}

local questTab = MainMainGUI:Tab{
	Name = "TP Quest Heads",
	Icon = "rbxassetid://553680044"
}

local mobTab = MainMainGUI:Tab{
	Name = "TP Mobs",
	Icon = "rbxassetid://4799887091"
}

local weaponTab = MainMainGUI:Tab{
	Name = "TP Weapons",
	Icon = "rbxassetid://7485051715"
}

local styleTab = MainMainGUI:Tab{
	Name = "TP Styles",
	Icon = "rbxassetid://6723742952"
}

questTab:Button{
	Name = "TP Big head boy",
	Description = nil,
	Callback = function()
            for _,v in pairs(game.Workspace:GetDescendants()) do
                if string.find(v.ClassName, "Model") and string.find(v.Name, 'Big head boy') then
                    v:FindFirstChild("Head").Anchored = true
                    v:FindFirstChild("Head").CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame*CFrame.new(0,0,-5)
                end
            end
    end
}

